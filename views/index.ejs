<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Truth Table</title>
</head>
<body>
  <h1>Truth Table for Z = A(B'C' + BC)</h1>
  <table border="1">
    <thead>
      <tr>
        <th>B</th>
        <th>C</th>
        <th>B'</th>
        <th>C'</th>
        <th>B'C' + BC</th>
      </tr>
    </thead>
    <tbody>
      <% truthTable.forEach(entry => { %>
        <tr>
          <td><%= entry.b %></td>
          <td><%= entry.c %></td>
          <td><%= entry.bPrime %></td>
          <td><%= entry.cPrime %></td>
          <td><%= entry.result %></td>
        </tr>
      <% }); %>
      
    </tbody>
  </table>
</body>
</html>
