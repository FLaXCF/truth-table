<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Truth Tables</title>
    <link rel="stylesheet" href="/styles.css"> <!-- Link to your CSS if you have any -->
</head>
<body>
    <h1>Truth Tables</h1>

    <div>
        <h2>Expression 1: ¬p ∧ (p ∨ q) → q</h2>
        <table>
            <thead>
                <tr>
                    <th>p</th>
                    <th>q</th>
                    <th>Term1 (¬p ∧ (p ∨ q))</th>
                    <th>Result ((¬p ∧ (p ∨ q)) → q)</th>
                </tr>
            </thead>
            <tbody>
                <% truthTables.expression1.forEach(row => { %>
                    <tr>
                        <td><%= row.p %></td>
                        <td><%= row.q %></td>
                        <td><%= row.term1 %></td>
                        <td><%= row.result %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <div>
        <h2>Expression 2: (p → q) ∧ (q → r) → (p → r)</h2>
        <table>
            <thead>
                <tr>
                    <th>p</th>
                    <th>q</th>
                    <th>r</th>
                    <th>Term1 ((p → q) ∧ (q → r))</th>
                    <th>Result</th>
                </tr>
            </thead>
            <tbody>
                <% truthTables.expression2.forEach(row => { %>
                    <tr>
                        <td><%= row.p %></td>
                        <td><%= row.q %></td>
                        <td><%= row.r %></td>
                        <td><%= row.term1 %></td>
                        <td><%= row.result %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <div>
        <h2>Expression 3: p ∧ (p → q) → q</h2>
        <table>
            <thead>
                <tr>
                    <th>p</th>
                    <th>q</th>
                    <th>Term1 (p ∧ (p → q))</th>
                    <th>Result</th>
                </tr>
            </thead>
            <tbody>
                <% truthTables.expression3.forEach(row => { %>
                    <tr>
                        <td><%= row.p %></td>
                        <td><%= row.q %></td>
                        <td><%= row.term1 %></td>
                        <td><%= row.result %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <div>
        <h2>Expression 4: (p ∨ q) ∧ (p → r) ∧ (q → r) → r</h2>
        <table>
            <thead>
                <tr>
                    <th>p</th>
                    <th>q</th>
                    <th>r</th>
                    <th>Term1 ((p ∨ q) ∧ (p → r) ∧ (q → r))</th>
                    <th>Result</th>
                </tr>
            </thead>
            <tbody>
                <% truthTables.expression4.forEach(row => { %>
                    <tr>
                        <td><%= row.p %></td>
                        <td><%= row.q %></td>
                        <td><%= row.r %></td>
                        <td><%= row.term1 %></td>
                        <td><%= row.result %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <div>
        <h2>Expression 5: a && !b || !c || (a && !b) || (b && !c)</h2>
        <table>
            <thead>
                <tr>
                    <th>a</th>
                    <th>b</th>
                    <th>c</th>
                    <th>Term1 (a && !b)</th>
                    <th>Term2 (!c)</th>
                    <th>Term3 (a && !b)</th>
                    <th>Term4 (b && !c)</th>
                    <th>Result</th>
                </tr>
            </thead>
            <tbody>
                <% truthTables.expression5.forEach(row => { %>
                    <tr>
                        <td><%= row.a %></td>
                        <td><%= row.b %></td>
                        <td><%= row.c %></td>
                        <td><%= row.term1 ? 1 : 0 %></td>
                        <td><%= row.term2 ? 1 : 0 %></td>
                        <td><%= row.term3 ? 1 : 0 %></td>
                        <td><%= row.term4 ? 1 : 0 %></td>
                        <td><%= row.result ? 1 : 0 %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <div>
        <h2>Expression 6: Complex Expression</h2>
        <table>
            <thead>
                <tr>
                    <th>a</th>
                    <th>b</th>
                    <th>c</th>
                    <th>Term1 (¬a ∧ b)</th>
                    <th>Term2 (¬c)</th>
                    <th>Term3 (¬a ∧ b) ∨ ¬c</th>
                    <th>Term4 (a ∧ ¬b) ∨ (¬b ∧ c)</th>
                    <th>Result</th>
                </tr>
            </thead>
            <tbody>
                <% truthTables.expression6.forEach(row => { %>
                    <tr>
                        <td><%= row.a %></td>
                        <td><%= row.b %></td>
                        <td><%= row.c %></td>
                        <td><%= row.aPrime ? 1 : 0 %></td>
                        <td><%= row.cPrime ? 1 : 0 %></td>
                        <td><%= row.term3 ? 1 : 0 %></td>
                        <td><%= row.term4 ? 1 : 0 %></td>
                        <td><%= row.result ? 1 : 0 %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

</body>
</html>
